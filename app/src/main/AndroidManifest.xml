<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chatproject5">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".QnaContentActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".EditAddressActivity" />
        <activity
            android:name=".WebViewActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AlbumActivity" />
        <activity
            android:name=".PlusMenuActivity"
            android:theme="@style/Popup" />
        <activity android:name=".QnaActivity" />
        <activity android:name=".MyPointActivity" />
        <activity android:name=".MyBodyActivity" />
        <activity android:name=".MyOrderListActivity" />
        <activity
            android:name=".AddressActivity" />
        <activity android:name=".MyInfoActivity" />
        <activity
            android:name=".MyProfileActivity"
            android:theme="@style/Popup" />
        <activity
            android:name=".SearchActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Chat_roomActivity" />
        <activity android:name=".Sign_upActivity" />
        <activity android:name=".Search_userInfoActivity" />
        <activity android:name=".EntranceActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>